{
    "name": "silver-hasher",
    "version": "0.0.2",
    "description": "Greatest Functions and Approaches",
    "scripts": {
        "test": "mocha -r ts-node/register src/**/*.spec.ts  --reporter spec",
        "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
        "build": "webpack"
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/onexample/silver-hasher.git"
    },
    "keywords": [
        "TypeScript"
    ],
    "author": "Vladyslav",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/onexample/silver-hasher/issues"
    },
    "homepage": "https://github.com/onexample/silver-hasher#readme",
    "devDependencies": {
        "@types/mocha": "^2.2.48",
        "@types/node": "^6.14.7",
        "@types/sinon": "^2.3.7",
        "coveralls": "^2.13.3",
        "istanbul": "^0.4.5",
        "mocha": "^3.5.3",
        "mocha-lcov-reporter": "^1.3.0",
        "nyc": "^11.9.0",
        "sinon": "^2.3.8",
        "source-map-support": "^0.4.18",
        "ts-loader": "^2.3.7",
        "ts-node": "^3.2.0",
        "typescript": "~2.2.2",
        "webpack": "^3.12.0"
    },
    "main": "src/index.ts",
    "dependencies": {
        "babel": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-register": "^6.26.0",
        "jsdoc": "^3.6.3",
        "mocha-lcov-reporter": "^1.3.0",
        "sinon": "^2.4.1",
        "ts-node": "^3.3.0",
        "tsconfig-paths": "^3.8.0",
        "typedoc": "^0.7.2"
    },
    "engines": {
        "node": ">=7.3.0"
    }
}
