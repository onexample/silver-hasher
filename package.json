{
    "name": "silver-hasher",
    "version": "0.0.1",
    "description": "Greatest Functions and Approaches",
    "scripts": {
        "test": "./node_modules/.bin/mocha -r ts-node/register src/**/*.spec.ts  --reporter spec",
        "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
        "build": "./node_modules/.bin/webpack"
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/onexample/silver-hasher.git"
    },
    "keywords": [
        "JavaScript"
    ],
    "author": "Vladyslav",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/onexample/silver-hasher/issues"
    },
    "homepage": "https://github.com/onexample/silver-hasher#readme",
    "devDependencies": {
        "@types/mocha": "^2.2.41",
        "@types/node": "~6.0.60",
        "@types/sinon": "^2.2.41",
        "coveralls": "^2.13.1",
        "istanbul": "^0.4.5",
        "mocha": "^3.4.2",
        "mocha-lcov-reporter": "^1.3.0",
        "nyc": "^11.1.0",
        "sinon": "^2.3.8",
        "source-map-support": "^0.4.15",
        "ts-loader": "^2.2.2",
        "ts-node": "^3.2.0",
        "typescript": "~2.2.2",
        "webpack": "^3.1.0"
    }
}
